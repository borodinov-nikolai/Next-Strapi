{
  "kind": "collectionType",
  "collectionName": "devices",
  "info": {
    "singularName": "device",
    "pluralName": "devices",
    "displayName": "Device",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "devices"
    },
    "description": {
      "type": "richtext"
    }
  }
}
